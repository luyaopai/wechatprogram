/* components/list/list.wxss */
.container {
  height: 200rpx;
  padding: 10rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #cccccc;
}

.left {
  width:68%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.top {
  font-weight: 600;
  font-size: 36rpx;
}

.bot {
  color: #c1c1c1;
  font-size: 28rpx;
}

.right {
  width: 30%;
  text-align: center;
  
}

.right image {
  width: 90%;
  height: 90%;
  margin-top: 4px;
}
